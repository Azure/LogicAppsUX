{
  "definition": {
    "$schema": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#",
    "actions": {
      "Init_all": {
        "inputs": {
          "variables": [
            {
              "name": "testVar",
              "type": "string",
              "value": "@{body('Create_a_goal')['id']},@{body('Create_a_goal_2')['id']},"
            }
          ]
        },
        "runAfter": {},
        "type": "InitializeVariable"
      },
      "Compose": {
        "inputs": "a",
        "runAfter": {
          "Init_all": ["SUCCEEDED"],
          "Http": ["SUCCEEDED"]
        },
        "type": "Compose"
      },
      "Execute_a_navigation_plan": {
        "inputs": {
          "a": "A"
        },
        "runAfter": {
          "Init_all": ["SUCCEEDED"]
        },
        "type": "Compose"
      },
      "Initialize_test_var": {
        "inputs": {
          "variables": [
            {
              "name": "testVar",
              "type": "string",
              "value": "@{body('Create_a_goal')['id']},@{body('Create_a_goal_2')['id']},"
            }
          ]
        },
        "runAfter": {
          "Init_all": ["SUCCEEDED"]
        },
        "type": "InitializeVariable"
      },
      "Response": {
        "type": "Response",
