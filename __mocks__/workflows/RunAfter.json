        },
        "operationOptions": "Asynchronous",
        "runtimeConfiguration": {
          "secureData": {
            "properties": ["inputs"]
          }
        },
        "trackedProperties": {
          "dfd": "fdfdf"
        }
      },
      "Terminate_2": {
        "inputs": {
          "runError": {
            "code": "CreateAndGetGoalFailed",
            "message": "Created goal does not match expected goal"
          },
          "runStatus": "Failed"
        },
        "runAfter": {
          "Response": ["Failed"],
          "Initialize_test_var": ["SUCCEEDED", "Skipped"]
        },
        "type": "Terminate"
      }
    },
    "contentVersion": "1.0.0.0",
    "outputs": {},
    "triggers": {
