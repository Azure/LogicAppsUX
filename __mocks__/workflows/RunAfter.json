        "inputs": {
          "statusCode": 200,
          "headers": "@triggerBody()",
          "body": "@outputs('Execute_a_navigation_plan')?['outputParameters']?['balance']"
        },
        "runAfter": {
          "Compose": ["SUCCEEDED"],
          "Initialize_test_var": ["SUCCEEDED", "Skipped"],
