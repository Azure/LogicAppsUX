    "prism-react-renderer": "^1.2.1",
    "react": "17.0.2",
    "react-dnd": "^14.0.4",
    "react-dnd-accessible-backend": "^1.0.0",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dnd-multi-backend": "^6.0.2",
    "react-dom": "17.0.2",
    "react-flow-renderer": "^9.6.7",
    "react-intl": "^5.20.10",
    "react-redux": "7.2.5",
    "react-syntax-highlighter": "^15.4.4",
    "react-use": "^17.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "to-title-case": "^1.0.0",
    "tslib": "^2.0.0",
    "web-worker": "^1.1.0",
    "worker-loader": "^3.0.8"
