{
  "name": "microsoft-logic-apps",
  "version": "0.0.23",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "start:storybook": "nx run designer:storybook",
    "build": "nx run designer:build",
    "build:all": "nx run-many --all --target=build",
    "test": "nx affected --target=test",
    "test:all": "nx run-many --all --target=test",
    "test:e2e": "nx run designer-e2e:e2e",
    "test:e2e:ci": "nx run designer-e2e:e2e:ci",
    "build-storybook": "NODE_ENV=production nx run designer:build-storybook",
    "lint": "nx affected --target=lint",
    "prepare": "husky install",
    "pretty": "nx format:write --uncommitted",
    "extract": "formatjs extract \"libs/**/*.ts*\" --ignore \"libs/**/*.{d,test,spec}.{ts,tsx}\" --out-file Localize/lang/strings.json --format ./format.js",
    "compile:loc": "formatjs compile-folder --ast --format ./format.js \"Localize/lang\" \"Localize/compiled-lang/\" ",
    "compile:loc:psuedo": "formatjs compile \"Localize/lang/strings.json\" --ast --format ./format.js --out-file \"Localize/compiled-lang/strings.en-XA.json\" --pseudo-locale en-XA",
    "build:resolution-service": "nx run services-resolution-service:build",
    "build:utils": "nx run utils:build",
    "bump": "standard-version --no-verify",
    "postinstall": "tsc -p ./tools/executors/web/tsconfig.json"
  },
  "private": true,
  "dependencies": {
    "@fluentui/azure-themes": "^8.1.63",
