{
  "version": 2,
  "projects": {
    "designer": {
      "root": "libs/designer",
      "sourceRoot": "libs/designer/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:rollup",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/designer",
            "tsConfig": "libs/designer/tsconfig.lib.json",
            "project": "libs/designer/package.json",
            "entryFile": "libs/designer/src/index.ts",
            "external": [
              "react/jsx-runtime"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "extractCss": true,
            "assets": [
              {
                "glob": "libs/designer/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/designer/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/designer"
          ],
          "options": {
            "jestConfig": "libs/designer/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/designer/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/designer",
            "config": {
              "configFolder": "libs/designer/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "designer-e2e": {
      "root": "apps/designer-e2e",
      "sourceRoot": "apps/designer-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/designer-e2e/cypress.json",
            "devServerTarget": "designer:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "designer:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/designer-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "designer-standalone": {
      "root": "apps/designer-standalone",
      "sourceRoot": "apps/designer-standalone/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/designer-standalone",
            "index": "apps/designer-standalone/src/index.html",
            "main": "apps/designer-standalone/src/main.tsx",
            "polyfills": "apps/designer-standalone/src/polyfills.ts",
            "tsConfig": "apps/designer-standalone/tsconfig.app.json",
            "assets": [
              "apps/designer-standalone/src/favicon.ico",
              "apps/designer-standalone/src/assets"
            ],
            "styles": [
              "apps/designer-standalone/src/styles.less"
            ],
            "scripts": [],
            "statsJson": true,
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/designer-standalone/src/environments/environment.ts",
                  "with": "apps/designer-standalone/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "designer-standalone:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "designer-standalone:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/designer-standalone/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/designer-standalone"
          ],
          "options": {
            "jestConfig": "apps/designer-standalone/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "designer-standalone-e2e": {
      "root": "apps/designer-standalone-e2e",
      "sourceRoot": "apps/designer-standalone-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/designer-standalone-e2e/cypress.json",
            "devServerTarget": "designer-standalone:serve",
            "tsConfig": "apps/designer-standalone-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "designer-standalone:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/designer-standalone-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "services-resolution-service": {
      "root": "libs/services/resolution-service",
      "sourceRoot": "libs/services/resolution-service/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/services/resolution-service",
            "main": "libs/services/resolution-service/src/index.ts",
            "tsConfig": "libs/services/resolution-service/tsconfig.lib.json",
            "assets": [
              "libs/services/resolution-service/*.md"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/services/resolution-service/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/services/resolution-service"
          ],
          "options": {
            "jestConfig": "libs/services/resolution-service/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/utils",
            "main": "libs/utils/src/index.ts",
            "tsConfig": "libs/utils/tsconfig.lib.json",
            "assets": [
              "libs/utils/*.md"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/utils/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/utils"
          ],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  }
}