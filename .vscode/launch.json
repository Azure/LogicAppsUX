{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Run designer vscode extension in dev mode",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/apps/vs-code-designer/dist"],
      "trace": false,
      "internalConsoleOptions": "openOnFirstSessionStart",
      "outFiles": ["${workspaceFolder}/apps/vs-code-designer/dist/*.js"],
      "preLaunchTask": "npm: build:extension",
      "env": {
        "DEBUGTELEMETRY": "v"
      }
    },
    {
      "name": "Debug e2e vscode extension tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/extest",
      "args": ["setup-and-run", "${workspaceFolder}/out/ui-test/*.js", ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
