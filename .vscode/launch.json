{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Run Designer Extension in Dev Mode",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/apps/vs-code-designer/dist"],
      "trace": false,
      "internalConsoleOptions": "openOnFirstSessionStart",
      "outFiles": ["${workspaceFolder}/apps/vs-code-designer/dist/*.js"],
      "preLaunchTask": "npm: build:extension",
      "env": {
        "DEBUGTELEMETRY": "v"
      }
    },
            {
      "name": "VSCode E2E",
      "type": "node",
      "request": "launch",
      "trace": false,
      "program": "${workspaceFolder}/apps/vs-code-designer/dist/e2e/runTest.js",
      "preLaunchTask": "npm: vscode-e2e",
      "env": {
        "DEBUGTELEMETRY": "v"
      }
    },
        {
      "name": "VSCode E2E extensionhost",
      "type": "extensionHost",
      "request": "launch",
      "trace": false,
      "runtimeExecutable": "${execPath}",
      "preLaunchTask": "npm: vscode-e2e",
      "env": {
        "DEBUGTELEMETRY": "v"
      },
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/apps/vs-code-designer/dist",
        "--extensionTestsPath=${workspaceFolder}/apps/vs-code-designer/dist/e2e/suite"
      ]
    }
  ]
}
