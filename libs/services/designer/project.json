{
  "root": "libs/services/designer",
  "sourceRoot": "libs/services/designer/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "./tools/executors/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/services/designer",
        "main": "libs/services/designer/src/index.ts",
        "tsConfig": "libs/services/designer/tsconfig.lib.json",
        "project": "libs/services/designer/package.json",
        "entryFile": "libs/services/designer/src/index.ts",
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "format": ["cjs", "esm"],
        "assets": [
          {
            "glob": "libs/services/designer/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/services/designer/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/services/designer"],
      "options": {
        "jestConfig": "libs/services/designer/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
