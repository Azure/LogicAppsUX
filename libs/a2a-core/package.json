{
  "name": "@microsoft/logicAppsChat",
  "version": "0.1.0",
  "description": "React SDK for building chat interfaces that connect to AI agents",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./styles.css": {
      "import": "./dist/react/styles.css",
      "require": "./dist/react/styles.css"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "check": "npm run typecheck && npm run test -- --run && npm run build",
    "prepublishOnly": "npm run check"
  },
  "keywords": [
    "a2a",
    "a2a-protocol",
    "ai",
    "agents",
    "ai-agents",
    "sdk",
    "browser",
    "typescript",
    "react",
    "chat",
    "streaming"
  ],
  "author": "Travis Vu",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/events": "^3.0.3",
    "@types/node": "^24.0.12",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.2.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "postcss-modules": "^6.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^6.4.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=19.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@fluentui/react-components": "^9.66.7",
    "@fluentui/react-icons": "^2.0.306",
    "eventemitter3": "^5.0.1",
    "idb": "^8.0.3",
    "immer": "^10.1.1",
    "marked": "^9.0.0",
    "marked-highlight": "^2.0.0",
    "p-retry": "^6.2.1",
    "prismjs": "^1.29.0",
    "zod": "^4.0.2",
    "zustand": "^4.4.0"
  },
  "sideEffects": [
    "*.css",
    "**/*.css"
  ]
}
