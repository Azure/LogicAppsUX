exports[`recommendation panel renders operation card 1`] = `
<div
  className="msla-operation-card"
>
  <div>
    <div
      className="msla-card-title-container"
    >
      <Memo(Image)
        alt="logo for Azure Blob Storage"
        className="msla-card-logo"
        imageFit={1}
        src="https://connectoricons-prod.azureedge.net/releases/v1.0.1563/1.0.1563.2732/azureblob/icon.png"
      />
    </div>
    <div
      className="msla-card-title-container"
    >
      <Text
        className="msla-card-title"
      >
        When a blob is added or modified (properties only) (V2)
      </Text>
      <button
        onClick={[Function]}
      >
        Select
      </button>
    </div>
  </div>
  <Text
    className="msla-card-description"
  >
    This operation triggers a flow when one or more blobs are added or modified in a container. This trigger will only fetch the file metadata. To get the file content, you can use the \\"Get file content\\" operation. The trigger does not fire if a file is added/updated in a subfolder. If it is required to trigger on subfolders, multiple triggers should be created.
  </Text>
  <div
    className="msla-tag-container"
  >
    <Text
      className="msla-tag"
    >
      Azure Blob Storage
    </Text>
    <Text
      className="msla-tag"
    >
      Trigger
    </Text>
    <Text
      className="msla-tag"
    >
      Azure
    </Text>
  </div>
</div>
`;
