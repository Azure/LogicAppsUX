{
  "name": "@microsoft/vscode-extension-logic-apps",
  "version": "3.3.0",
  "license": "MIT",
  "type": "module",
  "module": "build/lib/index.esm.js",
  "main": "build/lib/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./build/lib/index.mjs",
      "default": "./build/lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12"
  },
  "files": [
    "build/lib/**/*",
    "src"
  ],
  "scripts": {
    "build:lib": "tsup && tsc --emitDeclarationOnly && node ./build-finish.js",
    "test:lib": "vitest --retry=3"
  },
  "dependencies": {
    "@microsoft/vscode-azext-azureappservice": "0.8.1",
    "@microsoft/vscode-azext-utils": "0.4.6",
    "@microsoft/logic-apps-shared": "workspace:*",
    "react-intl": "6.3.0",
    "react-query": "3.39.3",
    "axios": "1.6.0",
    "@apidevtools/swagger-parser": "10.0.3",
    "reactflow": "11.8.2",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@types/vscode": "1.76.0",
    "@types/vscode-webview": "1.57.1"
  }
}
