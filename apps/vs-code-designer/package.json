        "@microsoft/logic-apps-shared": "workspace:*",
        "@types/fs-extra": "^11.0.4",
        "@types/vscode": "1.76.0",
        "@types/vscode-webview": "1.57.1",
        "@vscode/test-electron": "2.3.0",
        "azure-storage": "^2.10.3",
