    "type": "module",
    "scripts": {
        "build": "tsup src/main.ts --external=vscode",
        "vscode:designer:pack:step1": "cd ./dist/apps/vs-code-designer && npm install",
        "vscode:designer:pack:step2": "node extension-copy-svgs.js",
