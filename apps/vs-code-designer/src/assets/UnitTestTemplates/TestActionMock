            var actionMock<%= ActionNameCleaned %> = mockErrors != null && mockErrors.ContainsKey("<%= ActionName %>")
                ? new <%= ActionMockClassName %>(
                    status: <%= ActionMockStatus %>,
                    error: mockErrors["<%= ActionName %>"])
                : new <%= ActionMockClassName %>(
                    status: <%= ActionMockStatus %>,
                    outputs: mockOutputs != null && mockOutputs.ContainsKey("<%= ActionName %>")
                        ? (<%= ActionMockOutputClassName %>)mockOutputs["<%= ActionName %>"]
                        : new <%= ActionMockOutputClassName %>());