{
  "name": "microsoft-logic-app-vscode-extension",
  "displayName": "Azure Logic Apps (Standard)",
  "version": "0.2.22",
  "aiKey": "2fc394da-7c11-4c1c-b776-17763031415d",
  "main": "main.js",
  "engines": {
    "vscode": "^1.64.0"
  },
  "contributes": {
    "breakpoints": [
      {
        "language": "json"
      }
    ],
    "commands": [
      {
        "command": "logicAppsExtension.openDesigner",
        "title": "Open New Designer",
        "category": "Azure Logic Apps"
      },
      {
        "command": "logicAppsExtension.loadMore",
        "title": "Load More",
        "category": "Azure Logic Apps"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "azure",
          "title": "Azure",
          "icon": "resources/azure.svg"
        }
      ]
    },
    "views": {
      "azure": [
        {
          "id": "newAzLogicApps",
          "name": "New Logic Apps (Standard)",
          "when": "config.azureLogicAppsStandard.showExplorer == true"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "logicAppsExtension.openDesigner",
          "when": "view == newAzLogicApps && viewItem =~ /Remote;.*;Workflow;/i",
          "group": "1@2"
        }
      ],
      "explorer/context": [
        {
          "command": "logicAppsExtension.openDesigner",
          "when": "resourceFilename==workflow.json",
          "group": "navigation@3"
        }
      ],
      "commandPalette": [
        {
          "command": "logicAppsExtension.openDesigner",
          "when": "resourceFilename==workflow.json"
        },
        {
          "command": "azureLogicAppsStandard.loadMore",
          "when": "never"
        }
      ]
    }
  },
  "extensionDependencies": [
    "ms-vscode.azure-account"
  ],
  "activationEvents": [
    "onCommand:logicAppsExtension.openDesigner",
    "onCommand:logicAppsExtension.loadMore",
    "onView:newAzLogicApps",
    "onDebugInitialConfigurations"
  ],
  "galleryBanner": {
    "color": "#015cda",
    "theme": "dark"
  },
  "bugs": {
    "url": "https://github.com/Azure/logicapps/issues"
  },
  "homepage": "https://azure.microsoft.com/services/logic-apps/",
  "license": "SEE LICENSE IN LICENSE.md",
  "categories": [
    "Azure"
  ],
  "keywords": [
    "Azure",
    "Logic App",
    "serverless",
    "multi-root ready"
  ],
  "preview": false,
  "publisher": "ms-azuretools",
  "icon": "assets/logicapp.png",
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
