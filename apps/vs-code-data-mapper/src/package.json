{
  "name": "microsoft-data-mapper-vscode-extension",
  "version": "0.0.72",
  "main": "main.js",
  "engines": {
    "vscode": "^1.64.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "dataMapperExtension.start",
        "title": "Open Data Mapper"
      },
      {
        "command": "dataMapperExtension.createDataMapFile",
        "title": "Create new data map"
      },
      {
        "command": "dataMapperExtension.loadInputSchemaFile",
        "title": "Load input schema"
      },
      {
        "command": "dataMapperExtension.loadOutputSchemaFile",
        "title": "Load output schema"
      },
      {
        "command": "dataMapperExtension.loadDataMapFile",
        "title": "Load data map"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "dataMapperExtension.dataMapperMenu",
          "group": "navigation",
          "when": "resourceExtname in dataMapperExtension.supportedFileExts"
        }
      ],
      "dataMapperExtension.dataMapperMenu": [
        {
          "command": "dataMapperExtension.loadInputSchemaFile",
          "group": "navigation",
          "when": "resourceExtname in dataMapperExtension.supportedSchemaFileExts"
        },
        {
          "command": "dataMapperExtension.loadOutputSchemaFile",
          "group": "navigation",
          "when": "resourceExtname in dataMapperExtension.supportedSchemaFileExts"
        },
        {
          "command": "dataMapperExtension.loadDataMapFile",
          "group": "navigation",
          "when": "resourceExtname in dataMapperExtension.supportedDataMapFileExts"
        }
      ]
    },
    "submenus": [
      {
        "id": "dataMapperExtension.dataMapperMenu",
        "label": "Data Mapper"
      }
    ]
  },
  "extensionDependencies": [
    "ms-vscode.azure-account"
  ],
  "activationEvents": [
    "onCommand:dataMapperExtension.start"
  ],
  "galleryBanner": {
    "color": "#015cda",
    "theme": "dark"
  },
  "bugs": {
    "url": "https://github.com/Azure/logicapps/issues"
  },
  "homepage": "https://azure.microsoft.com/services/logic-apps/",
  "license": "SEE LICENSE IN LICENSE.md",
  "categories": [
    "Azure"
  ],
  "keywords": [
    "Azure",
    "Logic App",
    "serverless",
    "multi-root ready"
  ],
  "preview": false,
  "publisher": "ms-azuretools",
  "icon": "assets/logicapp.png",
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
