{
  "name": "microsoft-data-mapper-vscode-extension",
  "version": "0.0.75",
  "main": "main.js",
  "repository": "https://github.com/Azure/logic_apps_designer",
  "engines": {
    "vscode": "^1.64.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "azureDataMapper.openDataMapper",
        "title": "Open Data Mapper"
      },
      {
        "command": "azureDataMapper.createNewDataMap",
        "title": "Create new data map",
        "icon": "$(add)"
      },
      {
        "command": "azureDataMapper.loadInputSchemaFile",
        "title": "Load input schema"
      },
      {
        "command": "azureDataMapper.loadOutputSchemaFile",
        "title": "Load output schema"
      },
      {
        "command": "azureDataMapper.loadDataMapFile",
        "title": "Load data map"
      }
    ],
    "views": {
      "azure": [
        {
          "id": "azDataMapper",
          "name": "Data Mapper"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "azureDataMapper.createNewDataMap",
          "when": "view == azDataMapper",
          "group": "navigation@1"
        }
      ],
      "explorer/context": [
        {
          "submenu": "azureDataMapper.dataMapperMenu",
          "group": "navigation",
          "when": "resourceExtname in azureDataMapper.supportedFileExts"
        }
      ],
      "azureDataMapper.dataMapperMenu": [
        {
          "command": "azureDataMapper.loadInputSchemaFile",
          "group": "navigation",
          "when": "resourceExtname in azureDataMapper.supportedSchemaFileExts"
        },
        {
          "command": "azureDataMapper.loadOutputSchemaFile",
          "group": "navigation",
          "when": "resourceExtname in azureDataMapper.supportedSchemaFileExts"
        },
        {
          "command": "azureDataMapper.loadDataMapFile",
          "group": "navigation",
          "when": "resourceExtname in azureDataMapper.supportedDataMapFileExts"
        }
      ]
    },
    "submenus": [
      {
        "id": "azureDataMapper.dataMapperMenu",
        "label": "Data Mapper"
      }
    ]
  },
  "extensionDependencies": [
    "ms-vscode.azure-account"
  ],
  "activationEvents": [
    "onCommand:azureDataMapper.openDataMapper",
    "workspaceContains:host.json",
    "workspaceContains:**/.xsd",
    "workspaceContains:**/.xslt",
    "onView:azDataMapper",
    "onDebugInitialConfigurations"
  ],
  "galleryBanner": {
    "color": "#015cda",
    "theme": "dark"
  },
  "bugs": {
    "url": "https://github.com/Azure/logicapps/issues"
  },
  "homepage": "https://azure.microsoft.com/services/logic-apps/",
  "license": "SEE LICENSE IN LICENSE.md",
  "categories": [
    "Azure"
  ],
  "keywords": [
    "Azure",
    "Logic App",
    "serverless",
    "multi-root ready"
  ],
  "preview": false,
  "publisher": "ms-azuretools",
  "icon": "assets/logicapp.png",
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
